.clsprefix
  &-pagination
    width 100%
    height 30px
    text-align center

  &-pagination-page
    display inline-block
    width 30px
    height 30px
    font-size 14px
    background transparent
    border none
    outline none

  &-pagination-ellipsis
    font-weight bold

  &-pagination-current-page
    color: #616161;

  &-pagination-button
    color: #1564c0;
    user-select: none;
